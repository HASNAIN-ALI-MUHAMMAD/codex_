{
  "name": "commander_cli",
  "version": "1.0.0",
  "description": "CLI coding agent!",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "build:watch": "tsc --watch",
    "dev:ready": "nodemon --watch dist --delay 500ms",
    "dev": "concurrently \"npm run build:watch\" \"npm run dev:ready\"",
    "codex": "node dist/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "file": "node"
  },
  "bin": {
    "codex": "dist/index.js"
  },
